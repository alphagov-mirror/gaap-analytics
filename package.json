{
  "name": "gaap-analytics",
  "version": "2.0.0",
  "description": "Shared analytics tools for GaaP products",
  "main": "src/index.js",
  "browser": "lib/gaap-analytics.min.js",
  "scripts": {
    "precommit": "lint-staged",
    "compile": "mkdir -p lib && ./node_modules/browserify/bin/cmd.js src/index.js -t [ babelify ] -o lib/gaap-analytics.js && ./node_modules/uglify-js/bin/uglifyjs lib/gaap-analytics.js -c -m -o lib/gaap-analytics.min.js",
    "prepublish": "npm run compile",
    "test": "./node_modules/karma/bin/karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/gaap-analytics.git"
  },
  "keywords": [
    "nodejs",
    "js",
    "standardjs",
    "GOV.UK",
    "GaaP",
    "Google Analytics",
    "Event Tracking"
  ],
  "author": "Jon Heslop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphagov/gaap-analytics/issues"
  },
  "homepage": "https://github.com/alphagov/gaap-analytics#readme",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7",
              "ie >= 10"
            ]
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browser-env": "^3.2.4",
    "browserify": "^15.0.0",
    "chai": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "karma": "^2.0.0",
    "karma-browserify": "^5.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.2.0",
    "lint-staged": "^5.0.0",
    "mocha": "^4.1.0",
    "sinon": "^4.1.4",
    "standard": "^10.0.3",
    "uglify-js": "^3.3.5",
    "watchify": "^3.9.0"
  }
}
