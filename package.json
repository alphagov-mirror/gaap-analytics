{
  "name": "gaap-analytics",
  "version": "1.0.0",
  "description": "Shared analytics tools for GaaP products",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "compile": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/gaap-analytics.git"
  },
  "keywords": [
    "nodejs",
    "js",
    "standardjs",
    "GOV.UK",
    "GaaP",
    "Google Analytics",
    "Event Tracking"
  ],
  "author": "Jon Heslop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphagov/gaap-analytics/issues"
  },
  "homepage": "https://github.com/alphagov/gaap-analytics#readme",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {}
}
